<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.p6e.germ.security.mapper.P6eSecurityJurisdictionRelationGroupMapper">

    <select id="count" parameterType="com.p6e.germ.security.model.db.P6eSecurityJurisdictionRelationGroupDb"
            resultType="long">
        SELECT COUNT(*) FROM `p6e_germ_security_jurisdiction_relation_group`
        <trim prefix="WHERE" prefixOverrides="AND |OR">
            <if test="DB.gid != null">AND `gid` = #{DB.gid}</if>
            <if test="DB.jurisdictionId != null">AND `jurisdiction_id` = #{DB.jurisdictionId}</if>
        </trim>;
    </select>



    <select id="select" parameterType="com.p6e.germ.security.model.db.P6eSecurityJurisdictionRelationGroupDb"
            resultType="com.p6e.germ.security.model.db.P6eSecurityJurisdictionRelationGroupDb">
        SELECT
            `gid` AS `gid`,
            `jurisdiction_id` AS `jurisdictionId`,
            `jurisdiction_param` AS `jurisdictionParam`,
            `create_date` AS `createDate`,
            `update_date` AS `updateDate`,
            `operate` AS `operate`
        FROM
            `p6e_germ_security_jurisdiction_relation_group`
            <trim prefix="WHERE" prefixOverrides="AND |OR">
                <if test="DB.gid != null">AND `gid` = #{DB.gid}</if>
                <if test="DB.jurisdictionId != null">AND `jurisdiction_id` = #{DB.jurisdictionId}</if>
            </trim>
        LIMIT #{DB.page}, #{DB.size};
    </select>

    <delete id="delete" parameterType="com.p6e.germ.security.model.db.P6eSecurityJurisdictionRelationGroupDb">
        DELETE FROM `p6e_germ_security_jurisdiction_relation_group` WHERE `gid` = #{DB.gid};
    </delete>
</mapper>